{
  "name": "@tkskto/vue-component-analyzer",
  "version": "0.3.0",
  "description": "Analyze dependency tree for Vue.js SFC (Single File Component)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "vca": "bin/analyze.js"
  },
  "scripts": {
    "dev": "tsc -p src/server/tsconfig.json -w",
    "build": "tsc -p src/server/tsconfig.json && rollup --config",
    "lint": "eslint -c .eslintrc.json src",
    "cover": "npm run cover:test && npm run cover:report",
    "cover:test": "nyc npm run test:base -- --timeout 60000",
    "cover:report": "nyc report --reporter=html",
    "test": "npm run test:base -- --timeout 60000",
    "test:base": "ts-mocha ./test/specs/*.ts",
    "test:base:update": "ts-mocha ./test/specs/*.ts --update",
    "tmp": "node dist/index.js --dir test/fixture/declarationTest/importOne.vue -p 8888"
  },
  "keywords": [
    "Vue.js",
    "Vue",
    "component",
    "analyze",
    "tree"
  ],
  "author": {
    "name": "tkskto",
    "url": "https://github.com/tkskto/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkskto/vue-component-analyzer.git"
  },
  "license": "MIT",
  "files": [
    "bin",
    "dist",
    "types",
    "views"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "7.2.0",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "globby": "11.0.3",
    "mkdirp": "1.0.4",
    "opener": "1.5.2",
    "vue-eslint-parser": "7.6.0",
    "ws": "7.4.6"
  },
  "devDependencies": {
    "@mitsue/eslint-config": "4.0.1",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@types/express": "4.17.11",
    "@types/mocha": "8.2.2",
    "@types/node": "14.14.35",
    "@types/opener": "1.4.0",
    "@types/ws": "7.4.4",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "eslint": "7.27.0",
    "husky": "4.3.8",
    "lint-staged": "11.0.0",
    "mocha": "8.4.0",
    "nyc": "15.1.0",
    "rollup": "2.52.7",
    "rollup-plugin-license": "2.3.0",
    "ts-mocha": "8.0.0",
    "typescript": "4.3.2"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
