{
  "name": "@tkskto/vue-component-analyzer",
  "version": "0.8.0",
  "description": "Analyze dependency tree for Vue.js SFC (Single File Component)",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "bin": {
    "vca": "bin/analyze.js"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "run-p dev:*",
    "dev:typeScript": "tsc -p src/server/tsconfig.json -w",
    "dev:rollup": "rollup --config -w",
    "build": "tsc -p src/server/tsconfig.json && rollup --config",
    "lint": "eslint -c .eslintrc.json src",
    "cover": "npm run cover:test && npm run cover:report",
    "cover:test": "nyc npm run test:base -- --timeout 60000",
    "cover:report": "nyc report --reporter=html",
    "test": "npm run test:base",
    "test:base": "jest --config=jest.config.js --testTimeout=60000",
    "test:base:update": "jest --config=jest.config.js --updateSnapshot --testTimeout=60000",
    "tmp": "node bin/analyze.js --dir . -p 8888"
  },
  "keywords": [
    "Vue.js",
    "Vue",
    "component",
    "analyze",
    "tree"
  ],
  "author": {
    "name": "tkskto",
    "url": "https://github.com/tkskto/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkskto/vue-component-analyzer.git"
  },
  "license": "MIT",
  "files": [
    "bin",
    "dist",
    "types",
    "views"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@typescript-eslint/parser": "7.6.0",
    "commander": "12.0.0",
    "ejs": "3.1.9",
    "express": "4.19.2",
    "get-tsconfig": "4.7.3",
    "globby": "14.0.1",
    "mkdirp": "3.0.1",
    "opener": "1.5.2",
    "vue-eslint-parser": "9.4.2",
    "ws": "8.16.0"
  },
  "devDependencies": {
    "@mitsue/eslint-config": "4.1.0",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/ejs": "3.1.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/mkdirp": "2.0.0",
    "@types/node": "20.12.7",
    "@types/opener": "1.4.3",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "eslint": "9.2.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "npm-run-all2": "6.1.2",
    "nyc": "15.1.0",
    "rollup": "4.14.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.3"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  }
}
