{
  "root": true,
  "extends": [
    "@mitsue",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "multiline-comment-style": 0,
    "@typescript-eslint/no-var-requires": 0,
    "no-console": 0,
    "no-new": 0,
    "arrow-body-style": 0
  }
}
