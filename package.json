{
  "name": "@tkskto/vue-component-analyzer",
  "version": "0.9.1",
  "description": "Analyze dependency tree for Vue.js SFC (Single File Component)",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "bin": {
    "vca": "bin/analyze.js"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "run-p dev:*",
    "dev:typeScript": "tsc -p src/server/tsconfig.json -w",
    "dev:rollup": "rollup --config -w",
    "build": "tsc -p src/server/tsconfig.json && rollup --config",
    "lint": "eslint -c eslint.config.js src",
    "cover": "npm run cover:test && npm run cover:report",
    "cover:test": "nyc npm run test:base -- --timeout 60000",
    "cover:report": "nyc report --reporter=html",
    "test": "npm run test:base",
    "test:base": "jest --config=jest.config.js --testTimeout=60000",
    "test:base:update": "jest --config=jest.config.js --updateSnapshot --testTimeout=60000",
    "tmp": "node bin/analyze.js --dir . -p 8888"
  },
  "keywords": [
    "Vue.js",
    "Vue",
    "component",
    "analyze",
    "tree"
  ],
  "author": {
    "name": "tkskto",
    "url": "https://github.com/tkskto/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkskto/vue-component-analyzer.git"
  },
  "license": "MIT",
  "files": [
    "bin",
    "dist",
    "types",
    "views"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@typescript-eslint/parser": "8.23.0",
    "commander": "12.1.0",
    "ejs": "3.1.10",
    "express": "4.21.2",
    "get-tsconfig": "4.10.0",
    "globby": "14.1.0",
    "js-beautify": "1.15.2",
    "mkdirp": "3.0.1",
    "opener": "1.5.2",
    "vue-eslint-parser": "9.4.3",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@mitsue/eslint-config": "^7.0.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/ejs": "3.1.5",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.14",
    "@types/js-beautify": "1.14.3",
    "@types/mkdirp": "2.0.0",
    "@types/node": "^20.17.17",
    "@types/opener": "1.4.3",
    "@types/ws": "8.5.14",
    "@typescript-eslint/eslint-plugin": "8.23.0",
    "eslint": "^9.20.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "npm-run-all2": "^6.2.6",
    "nyc": "^17.1.0",
    "rollup": "^4.34.6",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "8.23.0"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  }
}
